<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.keutsa.prototype.view.LineChartController">
   <center>
      <LineChart fx:id="lineChart" alternativeColumnFillVisible="true" BorderPane.alignment="CENTER">
        <xAxis>
          <CategoryAxis fx:id="xAxis" label="Client Time" side="LEFT" />
        </xAxis>
        <yAxis>
          <CategoryAxis label="Connection Type" fx:id="yAxis" />
        </yAxis>
      </LineChart>
   </center>
</BorderPane>
