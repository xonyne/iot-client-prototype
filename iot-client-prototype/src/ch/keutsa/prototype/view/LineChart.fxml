<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.keutsa.prototype.view.LineChartController">
   <center>
      <LineChart fx:id="lineChart" alternativeColumnFillVisible="true" BorderPane.alignment="CENTER">
        <xAxis>
          <CategoryAxis label="Client Time" side="LEFT" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <CategoryAxis fx:id="yAxis" label="Connection Type" />
        </yAxis>
      </LineChart>
   </center>
   <bottom>
      <Button fx:id="otherClients" mnemonicParsing="false" onAction="#addClients" text="Other Clients" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin></Button>
   </bottom>
</BorderPane>
