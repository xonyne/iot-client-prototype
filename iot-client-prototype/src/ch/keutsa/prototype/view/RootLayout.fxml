<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.keutsa.prototype.view.RootLayoutController">
  <left>
    <Accordion fx:id="accordion" minWidth="130.0" prefHeight="-1.0" prefWidth="150.0" BorderPane.alignment="CENTER">
      <panes>
      </panes>
    </Accordion>
  </left>
  <top>
    <MenuBar BorderPane.alignment="CENTER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Close" onAction="#exitApplication" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" onAction="#showAbout" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</BorderPane>
